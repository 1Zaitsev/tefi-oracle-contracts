{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "description": "Owner operation to update the owner parameter",
      "type": "object",
      "required": [
        "update_owner"
      ],
      "properties": {
        "update_owner": {
          "type": "object",
          "required": [
            "owner"
          ],
          "properties": {
            "owner": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Owner operation to update the max_proxies_per_asset parameter",
      "type": "object",
      "required": [
        "update_max_proxies"
      ],
      "properties": {
        "update_max_proxies": {
          "type": "object",
          "required": [
            "max_proxies_per_asset"
          ],
          "properties": {
            "max_proxies_per_asset": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "register_source"
      ],
      "properties": {
        "register_source": {
          "type": "object",
          "required": [
            "asset_token",
            "proxy_addr"
          ],
          "properties": {
            "asset_token": {
              "type": "string"
            },
            "priority": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint8",
              "minimum": 0.0
            },
            "proxy_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Updates the priority paramter of an existing proxy",
      "type": "object",
      "required": [
        "update_source_priority"
      ],
      "properties": {
        "update_source_priority": {
          "type": "object",
          "required": [
            "asset_token",
            "priority",
            "proxy_addr"
          ],
          "properties": {
            "asset_token": {
              "type": "string"
            },
            "priority": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            },
            "proxy_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Remves an already whitelisted proxy",
      "type": "object",
      "required": [
        "remove_source"
      ],
      "properties": {
        "remove_source": {
          "type": "object",
          "required": [
            "asset_token",
            "proxy_addr"
          ],
          "properties": {
            "asset_token": {
              "type": "string"
            },
            "proxy_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "whitelist_proxy"
      ],
      "properties": {
        "whitelist_proxy": {
          "type": "object",
          "required": [
            "proxy_addr"
          ],
          "properties": {
            "proxy_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "remove_proxy"
      ],
      "properties": {
        "remove_proxy": {
          "type": "object",
          "required": [
            "proxy_addr"
          ],
          "properties": {
            "proxy_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
